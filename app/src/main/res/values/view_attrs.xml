<?xml version="1.0" encoding="utf-8"?>
<resources>



    <declare-styleable name="ColorSelectorView">
        <attr name="size" format="dimension" />
        <attr name="stroke_width" format="dimension" />
        <attr name="stroke_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="VitalAttribView">
        <attr name="max_value" format="integer" />
        <attr name="cur_value" format="float" />
        <attr name="box_size" format="dimension" />
        <attr name="box_padding" format="dimension" />
        <attr name="box_color" format="color" />
        <attr name="line_color" format="color" />
        <attr name="tick_color" format="color" />
        <attr name="tick_width" format="dimension" />
        <attr name="variant">
            <enum name="lp" value="0" />
            <enum name="ep" value="1" />
            <enum name="mp" value="1" />
        </attr>
    </declare-styleable>
</resources>